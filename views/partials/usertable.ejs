
<table width="100%">
  <thead>
    <tr>
      <th>ID</th>
      <th>Username</th>
      <th>Senha</th>
    </tr>
  </thead>
  <tbody>
    <% let i; for(i=0; i<dados.length; i++) { %>
    <tr>
      <td><%= dados[i].id %></td>
      <td><%= dados[i].username %></td>
      <td><%= dados[i].password %></td>
    </tr>
    <% } %>
  </tbody>
  <tfoot>
    <tr>
      <th colspan="3">
        <%- `Qtd. de Registros ${dados.length}` %>
      </th>
    </tr>
  </tfoot>
</table>
